    "redux-mock-store": "^1.5.4",
    "typescript": "^3.7.4"
  },
  "scripts": {
    "start:dev": "NODE_ENV=development parcel src/index.html -p 5000",
    "build": "parcel build src/index.html",
    "test": "jest",
    "lint:ts": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "lintPretty": "pretty-quick",
    "lint": "run-p --print-label lint:*",
    "pre-commit-hook": "run-s \"lintPretty\" \"lint:js --fix\" \"test\" \"git add\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "pretty-quick",
      "eslint --fix",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
